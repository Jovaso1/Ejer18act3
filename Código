Clase salario:
package ejemplo.ejer18.empleado;

/**
 *
 * @author USER
 */
public class Salario {
    public static double calcsalbruto(double horas_trabajadas, double valor_hora){
    double salario_bruto;
    salario_bruto = horas_trabajadas * valor_hora;
    return salario_bruto;
    }
    
    public static double calcsalneto(double horas_trabajadas, double valor_hora, double retencion){
    double salario_bruto;
    double salario_neto;
    salario_bruto = horas_trabajadas * valor_hora;
    salario_neto = salario_bruto - (salario_bruto*(retencion/100));
    return salario_neto;
    }
}
