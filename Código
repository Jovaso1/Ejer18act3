    private void botsalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        txtcodempleado.setText("");
        txtnombres.setText("");        
        txthortrabajadas.setText("");
        txtvalorhora.setText("");
        txtretencion.setText("");      
        txtcodigo.setText("");
        txtnombres1.setText("");
        txtsalbruto.setText("");
        txtsalneto.setText("");
    }                                        

