    private void botoncalcularActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
        double horas_trabajadas, valor_hora, retencion, salario_bruto, salario_neto;
        String nombre, cod_emple;
        
        cod_emple = String.valueOf(txtcodempleado.getText());
        nombre = String.valueOf(txtnombres.getText());        
        horas_trabajadas = Double.parseDouble(txthortrabajadas.getText());
        valor_hora = Double.parseDouble(txtvalorhora.getText());
        retencion = Double.parseDouble(txtretencion.getText());       
       
        salario_bruto = Salario.calcsalbruto(horas_trabajadas, valor_hora);
        salario_neto = Salario.calcsalneto(horas_trabajadas, valor_hora, retencion);       
        
        txtcodigo.setText(String.valueOf(cod_emple));
        txtnombres1.setText(nombre);
        txtsalbruto.setText(String.valueOf(salario_bruto));
        txtsalneto.setText(String.valueOf(salario_neto));
        
    }                                             

    private void botonsalirActionPerformed(java.awt.event.ActionEvent evt) {                                           
